# Copy this file to `.env` and fill in values as needed.
# Used by docker-compose.yml and app startup.

# Google AI Studio (Gemini) API key
GOOGLE_API_KEY=your_google_api_key_here

# Database URL (default: local SQLite file)
DATABASE_URL=sqlite:///./llm_evaluator.db

# Path to seed CSV for initial data population
SEED_CSV_PATH=mock_data.csv
